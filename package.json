    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-sass": "^4.0.2",
    "jam-icons": "^2.0.0",
    "jquery": "^3.3.1",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^3.1.0",
    "popper.js": "^1.14.5",
    "style-loader": "^0.19.0",
