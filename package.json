{
  "name": "devview",
  "version": "1.0.0",
  "description": "Devview project.",
  "main": "index.js",
  "scripts": {
    "start": "node ./app/server/index.js",
    "build": "gulp && webpack --mode production",
    "client": "gulp && opn http://localhost:8000 && webpack --mode development --watch",
    "server": "nodemon ./app/server/index.js",
    "heroku:logs": "heroku logs --tail",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "npm run server",
    "dev:client": "npm run client",
    "dev2": "npm-run-all --parallel dev2:*",
    "dev2:server": "npm start",
    "dev2:client": "npm run client",
    "heroku-prebuild-disabled": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev && npm run build",
    "heroku-postbuild-disabled": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;"
  },
  "author": "Author of Devview",
  "license": "MIT",
  "dependencies": {
    "alexa-app": "^4.2.3",
    "bcryptjs": "^2.4.3",
    "busboy": "^0.2.14",
    "busboy-body-parser": "^0.3.2",
    "connect-busboy": "0.0.2",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "pg": "^7.7.1",
    "react": "^16.6.3",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-toasts": "^2.0.14",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ssml-builder": "^0.4.3",
    "sweetalert-react": "^0.4.11",
    "teamwork-api": "^1.0.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.5",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "bootstrap": "^4.1.3",
    "browser-sync": "^2.26.3",
